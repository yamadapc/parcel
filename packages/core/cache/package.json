{
  "name": "@parcel/cache",
  "description": "Interface for defining caches and file-system, IDB and LMDB implementations.",
  "version": "2.12.0",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/parcel"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/parcel-bundler/parcel.git"
  },
  "main": "lib/index.js",
  "source": "src/index.js",
  "types": "index.d.ts",
  "engines": {
    "node": ">= 12.0.0"
  },
  "scripts": {
    "build-ts": "mkdir -p lib && flow-to-ts src/types.js > lib/types.d.ts",
    "check-ts": "tsc --noEmit index.d.ts",
    "clean": "rimraf ./lib",
    "build:babel": "babel --presets=@parcel/babel-preset src --out-dir lib"
  },
  "dependencies": {
    "@parcel/fs": "2.12.0",
    "@parcel/logger": "2.12.0",
    "@parcel/utils": "2.12.0",
    "lmdb": "2.8.5"
  },
  "peerDependencies": {
    "@parcel/core": "^2.12.0"
  },
  "devDependencies": {
    "idb": "^5.0.8",
    "@babel/core": "^7.22.11",
    "@babel/cli": "^7.22.11",
    "rimraf": "^5.0.5"
  },
  "browser": {
    "./src/IDBCache.js": "./src/IDBCache.browser.js",
    "./src/LMDBCache.js": false
  }
}
